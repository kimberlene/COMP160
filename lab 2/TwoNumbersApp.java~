public class TwoNumbersApp {
  public static void main (String args[]){
    double num1;
    double num2;
    double sum =0.0d;
    num1 = 8.5;
    num2 = 15.0;
    sum = num1+num2;
    System.out.println("First number is " + num1);
    System.out.println("Second number is " + num2);
      System.out.println("Sum is " + sum);
  }
}
      
    